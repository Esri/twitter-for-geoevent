<connectorDefinition label="Twitter Output Connector"
	name="twitter-out" type="outbound">
	<adapter uri="com.esri.geoevent.adapter.outbound/twitter/10.2.0" />
	<defaultName>twitter-out</defaultName>
	<description>Send Tweet status updates to Twitter</description>
	<properties>
		<advanced>
		
			<property label="URL" name="clientURL" default="https://stream.twitter.com/1.1/statuses/filter.json" source="transport" />
		
		</advanced>
		<hidden>
			<property label="Mode" name="mode" default="CLIENT" source="transport" />
			<property label="Use Long Polling" name="useLongPolling" default="true" source="transport" />
			<property label="Receive New Data Only" name="honorLastModified" default="true" source="transport" />
			<property label="Frequency (in seconds)" name="frequency" default="5" source="transport" />
			<property label="Post body MIME Type" name="postContentType" default="application/x-www-form-urlencoded" source="transport" />
			<property label="Acceptable MIME Types (Client Mode)" name="acceptableMimeTypesClientMode" default="application/x-www-form-urlencoded" source="transport" />
			<property label="HTTP Method" name="httpMethod" default="POST" source="transport" />
			<property label="Content Body" name="clientPostBody" source="transport" />
			<property label="Parameters" name="clientParameters" source="transport" />
			<property label="URL Proxy" name="clientURLProxy" source="transport" />
			<property label="Use URL Proxy" name="useClientURLProxy" default="false" source="transport" />
			<property label="Acceptable MIME Types (Server Mode)" name="acceptableMimeTypesServerMode" source="transport" />		</hidden>
		<shown>
	        <property name="consumerKey" label="ConsumerKey" source="transport" />
	        <property name="consumerSecret" label="ConsumerSecret" source="transport" />
	        <property name="accessToken" label="AccessToken" source="transport" />
	        <property name="accessTokenSecret" label="AccessTokenSecret" source="transport" />
		</shown>
	</properties>
	<transport uri="com.esri.ges.transport.outbound/Twitter/10.2.0" />
</connectorDefinition>